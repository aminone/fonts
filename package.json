{
  "name": "fonts",
  "main": "./lib/fonts",
  "version": "3.5.0",
  "description": "Lots of monospace fonts.",
  "repository": "https://github.com/braver/fonts",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "dependencies": {},
  "keywords": [
    "fonts",
    "monospace",
    "programming",
    "typography",
    "@fontface"
  ],
  "scripts": {
